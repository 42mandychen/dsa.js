{
  "extends": ["eslint:recommended", "plugin:import/errors", "plugin:import/warnings"],
  "parser": "babel-eslint",
  "rules": {
    "no-console": 0,
    "prefer-const": 1,
    "for-direction": 2,
    "getter-return": 2,
    "no-template-curly-in-string": 1,
    "array-callback-return": 1,
    "consistent-return": 1,
    "default-case": 1,
    "eqeqeq": 1,
    "no-else-return": 1,
    "no-empty-function": 1,
    "no-implicit-coercion": 1,
    "no-new": 1,
    "no-new-func": 1,
    "no-new-wrappers": 1,
    "no-param-reassign": 1,
    "no-return-assign": 1,
    "no-self-compare": 1,
    "no-unused-expressions": 1,
    "no-useless-return": 1,
    "radix": 1,
    "no-undef-init": 1,
    "no-use-before-define": 2,
    "array-bracket-spacing": 1,
    "keyword-spacing": [1,
      {
        "before": true,
        "after": true
      }
    ],
    "camelcase": 1,
    "func-style": [1, "declaration"],
    "no-array-constructor": 1,
    "no-lonely-if": 1,
    "no-multi-assign": 2,
    "no-new-object": 1,
    "no-tabs": 1,
    "no-trailing-spaces": 1,
    "spaced-comment": [1, "always"],
    "no-var": 1,
    "no-duplicate-imports": 1,
    "no-useless-computed-key": 1,
    "no-useless-constructor": 1,
    "prefer-arrow-callback": 1,
    "semi": 2
  },
  "env": {
    "node": true,
    "jest": true
  }
}
